<?xml version="1.0"?>
<info xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>secsignid</id>
    <name>SecSign ID</name>
    <summary>SecSign 2FA for your NextCloud environment</summary>
    <description><![CDATA[Use SecSign 2FA to authenticate your login to NextCloud]]></description>
    <version>0.0.1</version>
    <licence>agpl</licence>
    <author mail="bjoern@secsign.com" >Björn Plüster</author>
    <namespace>SecSignID</namespace>
    <category>security</category>
    <bugs>https://jirasecsignidplugin.seccommerce.biz/issues/</bugs>
    <dependencies>
        <nextcloud min-version="14" max-version="15"/>
    </dependencies>
    <settings>
        <personal>OCA\SecSignID\Settings\personal</personal>
    </settings>
    <two-factor-providers>
        <provider>OCA\SecSignID\Provider\TwoFactorTestProvider</provider>
    </two-factor-providers>
    <navigations>
        <navigation role="admin">
            <name>SecSign ID</name>
            <route>secsignid.page.index</route>
        </navigation>
    </navigations>
</info>
